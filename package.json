{
  "name": "aps",
  "version": "0.0.1",
  "description": "a time waster project",
  "main": "main.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "node main.js",
    "test": "node ./node_modules/mocha/bin/mocha --recursive ./tests/unit",
    "test-integration": "node ./node_modules/mocha/bin/mocha --recursive ./tests/integration",
    "debug": "node-debug main.js",
    "debug-test": "node-debug node_modules/mocha/bin/_mocha tests/unit/server-spec.js",
    "build": "npm run build-js && npm run build-css",
    "release": "npm run release-js && npm run release-css",
    "watch": "npm run watch-js & npm run watch-css",
    "build-css": "rework-npm web/client/css/index.css -o web/server/content/css/bundle.css",
    "build-js": "browserify --extension=.jsx --extension=.js web/client/main.jsx > web/server/content/js/bundle.js",
    "release-css": "rework-npm web/client/css/index.css | cleancss -o web/server/content/css/bundle.css",
    "release-js": "browserify --extension=.jsx --extension=.js web/client/main.jsx | uglifyjs > web/server/content/js/bundle.js",
    "watch-js": "watchify --extension=.jsx --extension=.js web/client/main.jsx -o web/server/content/js/bundle.js --debug --verbose",
    "watch-css": "nodemon -e css --ignore web/server/content/css/bundle.css --exec 'rework-npm web/client/css/index.css -o web/server/content/css/bundle.css'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcapuano328/aps.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "react",
    "material-ui",
    "oauth"
  ],
  "author": "jpc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcapuano328/aps/issues"
  },
  "homepage": "https://github.com/jcapuano328/aps#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "config": "^1.24.0",
    "cookie-parser": "^1.4.3",
    "easy-pbkdf2": "^0.1.1",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "file": "^0.2.2",
    "font-awesome": "^4.7.0",
    "history": "^4.4.0",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.1.0",
    "kerberos": "0.0.21",
    "lodash": "^4.16.6",
    "material-ui": "^0.16.1",
    "mongodb": "^2.2.11",
    "node-uuid": "^1.4.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fontawesome": "^1.3.1",
    "react-intl": "^2.1.5",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "clean-css": "^3.4.20",
    "commander": "^2.9.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "moment": "^2.15.2",
    "nodemon": "^1.11.0",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "sandboxed-module": "^2.0.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  }
}
